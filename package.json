{
  "name": "sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main/server",
  "scripts": {
    "dev": "tsnd  -r tsconfig-paths/register --respawn --transpile-only ./src/main/server",
    "start": "node .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rimraf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "format": "prettier -w .",
    "test": "jest --passWithNoTests --colors --runInBand",
    "test:unit": "npm run test -- -c jest-unit-config.js",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "npm run test -- -c jest-integration-config.js",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:staged": "npm run test -- --findRelatedTests --silent",
    "test:ci": "npm run test -- --coverage",
    "type:check": "rimraf ./dist && tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Gabriel dos Santos Oliveira <gabrielsantosoliveira951@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "fastify": "^4.23.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/supertest": "^2.0.12",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
